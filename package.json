{
  "name": "to-do-list",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint",
    "pret": "prettier --write .",
    "format": "eslint src --fix && prettier --write 'src/**/*.{js,jsx,ts,tsx,css,scss,md}'",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css,scss,md}": [
      "npm run format"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "3.4.2",
    "vite": "^6.0.5"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.6",
    "tailwindcss": "^4.0.6"
  }
}
